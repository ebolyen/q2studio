{
  "name": "q2studio",
  "version": "2017.7.0-dev0",
  "description": "A graphical user interface for QIIME 2.",
  "author": "QIIME 2 Development Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/qiime2/q2studio.git"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --config frontend/config/webpack.config.babel.js",
    "start": "cross-env NODE_ENV=production electron dist/main.js",
    "dev:main": "webpack --config frontend/config/webpack.config.babel.js --env.DEV_MAIN",
    "dev:renderer": "webpack-dev-server --config frontend/config/webpack.config.babel.js --env.DEV_RENDERER",
    "dev:start": "sleep 4 && cross-env NODE_ENV=development electron dist/main.dev.js",
    "dev": "npm run dev:main && concurrently -k \"npm run dev:start\" \"npm run dev:renderer\" || true"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "electron": "^1.6.11",
    "html-webpack-plugin": "^2.29.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "webpack": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "electron-devtools-installer": "^2.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack-dev-server": "^2.5.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "private": true
}
